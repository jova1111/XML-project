<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
 xmlns:sys="http://www.booking.com/sys"
 elementFormDefault="qualified"
   >
    <xs:import namespace="http://www.booking.com/sys" schemaLocation="semara.xsd"/>
    <xs:element name="Comment">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="sys:User"/>
                <xs:element ref="sys:Lodging"/>
            </xs:sequence>
            <xs:attribute name="text" type="xs:string"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
